{
    "name": "elsayed85/laravel-translation",
    "description": "A tool for managing all of your Laravel translations",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Joe Dixon",
            "email": "hello@Elsayed85.co.uk"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "laravel/legacy-factories": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Elsayed85\\Translation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elsayed85\\Translation\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elsayed85\\Translation\\TranslationServiceProvider",
                "Elsayed85\\Translation\\TranslationBindingsServiceProvider"
            ]
        }
    }
}